{
	"texture_manager": {
		"cdn_url": "http://static-cdn-jband.gree-apps.net/jband/",
		"texture_expiration_time": 60000,

		"texture_options": {
			"filter": "linear"
		}
	},

	"fonts": [
		{
			"name": "font1",
            "style": "bold",
            "characters": "@string/charset_basic_latin",
            "padding_x": 10,
            "padding_y": 10,
            "shadow_dx": 5,
            "shadow_dy": 5,
            "shadow_radius": 5.5,
            "size": 50,
            "stroke_color": "#FFFF0000",
            "stroke_size": 55,
            "typeface": "fonts/foo.ttf",
            "color": "#FFFFFF00"
		},

		{
			"name": "font2",
            "style": "bold",
            "characters": "@string/charset_basic_latin",
            "padding_x": 5,
            "padding_y": 5,
            "shadow_radius": 5.5,
            "size": 30,
            "stroke_color": "#FF333333",
            "color": "#FFFFFFFF"
		}
	],

	"animators": [
		{
			"name": "ani_resize",
			"type": "resize",
			"width_from": 400,
			"width_to": 600,
			"height_from": 600,
			"height_to": 400,
			"duration": 2000,
			"loop_mode": "reverse"
		},

		{
			"name": "ani_fade_in",
			"type": "alpha",
			"from": 0,
			"to": 1,
			"interpolation": "decelerate",
			"duration": 3000
		},

		{
			"name": "ani_alpha",
			"type": "alpha",
			"from": 1,
			"to": 0.5,
			"duration": 500,
			"interpolation": "decelerate",
			"loop_mode": "reverse"
		},

		{
			"name": "ani_color",
			"type": "color",
			"from": "#FFFFFFFF",
			"to": "#FFFFFF00",
			"duration": 1000,
			"interpolation": "decelerate",
			"loop_mode": "reverse"
		},

		{
			"name": "ani_rotate",
			"type": "rotate",
			"from": 0,
			"to": 360,
			"duration": 2000,
			"loop_mode": "repeat"
		},

		{
			"name": "ani_heart_beat",
			"type": "scale",
			"from": 1,
			"to": 1.25,
			"duration": 500,
			"loop_mode": "repeat"
		}
	]
}

